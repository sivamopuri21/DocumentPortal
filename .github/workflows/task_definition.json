{
  "family": "documentportaltd",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::654455899822:role/DocumentPortalRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "documentportalliveclass",
      "image": "654455899822.dkr.ecr.ap-south-1.amazonaws.com/documentportalliveclass",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "document-portal-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "LANGCHAIN_PROJECT",
          "value": "DOCUMENT PORTAL"
        }
      ],
      "secrets": [
        {
          "name": "GROQ_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:654455899822:secret:api_key-5yv7sD:GROQ_API_KEY"
        },
        {
          "name": "HF_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:654455899822:secret:api_key-5yv7sD:HF_TOKEN"
        },
        {
          "name": "GOOGLE_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:654455899822:secret:api_key-5yv7sD:GOOGLE_API_KEY"
        },
        {
          "name": "LANGCHAIN_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-south-1:654455899822:secret:api_key-5yv7sD:LANGCHAIN_API_KEY"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "false",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    }
  ]
}